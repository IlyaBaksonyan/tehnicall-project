{
	"name": "tehnicall-project",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only",
		"buildTest": "run-p type-check build-onlyTest",
		"preview": "vite preview",
		"build-only": "vite build --base=/tehnicall-project/",
		"build-onlyTest": "vite build --base=./",
		"type-check": "vue-tsc --noEmit",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
	},
	"dependencies": {
		"leader-line-new": "^1.1.9",
		"leader-line-vue": "^2.1.1",
		"vue": "^3.2.45",
		"vue-router": "^4.1.6",
		"vue3-leaderline": "^1.2.11"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.1.4",
		"@types/node": "^18.11.18",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.0",
		"@vue/tsconfig": "^0.1.3",
		"eslint": "^8.7.0",
		"eslint-plugin-vue": "^9.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"sass": "^1.57.1",
		"typescript": "~4.7.4",
		"vite": "^4.0.0",
		"vue-eslint-parser": "^8.0.0",
		"vue-tsc": "^1.0.24"
	}
}
